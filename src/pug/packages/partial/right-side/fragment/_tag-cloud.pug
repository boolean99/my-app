+b('side-bar-panel', { t: 'section', e: 'side-bar', m: 'tag-cloud' })
  +e('heading', { t: 'h1', m: 'type-outer' }) TAG CLOUD
  +b('toggle-button-input', { t: 'input' })(type='checkbox' id='tag-cloud')
  +b('toggle-button-label', { t: 'label' })(for='tag-cloud')
    include ../../hamburger/_hamburger
  +e('wrapper', { t: 'div' })
    +b('tag-cloud', { t: 'ul', e: '&' })
      each key in ['HTML', 'CSS', 'PUG', 'SASS', 'JAVASCRIPT', 'ES2015', 'JSON', 'AJAX', 'GULP', 'NPM', 'NODEJS', 'EXPRESS', 'SOKET.IO']
        +e('item', { t: 'li' })= key