if fontawesome
  script(src="https://use.fontawesome.com/5e23f07cdd.js" async)
script
  include ../others/js/browser-check.js
if js
  each key in jsFileName
    if(key.constructor.toString().toLowerCase().indexOf('array') > -1)
      - var attributes = {}
      
      -for(var i = 0; i < key.length - 1; i++)
        - attributes[key[i + 1].slice(0, key[i + 1].indexOf(':'))] = key[i + 1].slice(key[i + 1].indexOf(':') + 1) == 'true' ? true : key[i + 1].slice(key[i + 1].indexOf(':') + 1)

      
      script(src= key[0] id= key[0].slice(key[0].lastIndexOf('/') + 1, key[0].indexOf('.')))&attributes(attributes)
    else
      script(src= key id= key.slice(key.lastIndexOf('/') + 1, key.indexOf('.')))
if reloadJs
  script(src='/reload/reload.js')
if inlineJavascript
  script
    include ../others/js/script.js
